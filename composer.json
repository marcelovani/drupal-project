{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Marcelo Vani",
            "email": "no@reply.com",
            "homepage": "https://www.marcelovani.eu",
            "role":  "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.drupal-composer.org"
        },
        {
            "type": "package",
            "package": {
                "name": "backupmigrate/core",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/backupmigrate/backup_migrate_core",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "drupal/google_site_search",
                "type": "drupal-module",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/WondrousLLC/google_site_search.git",
                    "type": "git",
                    "reference": "origin/master"
                }
            }

        },
         {
            "type": "package",
            "package": {
                "name": "drupal/flood_controls",
                "type": "drupal-module",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/TeamGoblin/Flood-Control.git",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.0.20",
        "drupal-composer/drupal-scaffold": "^2.0.1",
        "cweagans/composer-patches": "~1.0",
        "drupal/core": "~8.3",
        "drush/drush": "~8.0",
        "drupal/console": "~1.0",
        "drupal/blazy": "~1.0",
        "drupal/classy_paragraphs": "~1.0",
        "drupal/coffee": "~1.0",
        "drupal/colorbox": "~1.0",
        "drupal/colorbox_load": "~1.0",
        "drupal/config_update": "~1.1",
        "drupal/ctools": "~3.0",
        "drupal/devel": "*",
        "drupal/ds": "~2.5",
        "drupal/entity_reference_revisions": "~1.0",
        "drupal/features": "~3.0",
        "drupal/field_group": "~1.0",
        "drupal/fpa": "~2.0",
        "drupal/layout_plugin": "~1.0",
        "drupal/linked_field": "1.x-dev",
        "drupal/linkit": "~4.0",
        "drupal/masquerade": "~1.0",
        "drupal/metatag": "1.0.0-beta9",
        "drupal/ng_lightbox": "~1.0",
        "drupal/paragraphs": "~1.0",
        "drupal/pathauto": "~1.0",
        "drupal/slick": "~1.0",
        "drupal/slick_views": "~1.0",
        "drupal/smart_trim": "~1.0",
        "drupal/stage_file_proxy": "~1.0",
        "drupal/token": "~1.0",
        "drupal/video_embed_field": "~1.0",
        "drupal/config_tools": "~1.0",
        "drupal/contact_block": "~1.2",
        "drupal/redis": "~1.0",
        "drupal/google_site_search": "*",
        "drupal/easychart": "~3.3",
        "drupal/rabbit_hole": "~1.0",
        "drupal/twig_tweak": "~1.0",
        "drupal/eu_cookie_compliance": "~1.0-beta7",
        "drupal/contact_storage": "~1.0",
        "drupal/backup_migrate": "~4.0.0-alpha1",
        "drupal/readonlymode": "~1.0",
        "drupal/redirect": "~1.0",
        "drupal/entity_embed": "~1.0",
        "drupal/embed": "~1.0",
        "drupal/flood_unblock": "~1.0",
        "drupal/flood_controls": "*",
        "drupal/libraries": "~3.0",
        "drupal/simplify": "^8.1",
        "drupal/admin_menu": "8.*",
        "drupal/rules": "8.*",
        "drupal/google_analytics": "^8.2",
        "drupal/taxonomy_menu": "8.*",
        "drupal/better_exposed_filters": "8.*",
        "drupal/votingapi": "8.*",
        "drupal/feeds": "8.*",
        "drupal/views_slideshow": "8.*",
        "drupal/webform": "8.*",
        "drupal/oauth": "8.*",
        "drupal/job_scheduler": "8.*"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8",
        "drupal/twig_xdebug": "~1.0",
        "drupal/drupal-extension": "~3.0",
        "squizlabs/php_codesniffer": ">=2.5.1",
        "drupal/coder": "~8.0",
        "andrewholgate/drupalstrict": "*",
        "pheromone/phpcs-security-audit": "dev-master",
        "phpmetrics/phpmetrics": "*",
        "mayflower/php-codebrowser": "~1.1",
        "pdepend/pdepend" : "~2.2.4",
        "phpmd/phpmd" : "~2.4.2",
        "phploc/phploc": "~3.0",
        "sebastian/phpcpd": "*"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        }
    },
    "patches": {
            "drupal/config_tools": {
                "Error on directory definition https://www.drupal.org/node/2499831": "https://www.drupal.org/files/issues/error_on_directory_definition-2499831-6.patch"
            },
            "drupal/rabbit_hole": {
                "TypeError when editing content type settings": "https://www.drupal.org/files/issues/typeerror-on-ct-settings-2780501-2.patch"
            },
            "drupal/google_site_search": {
                "Fixed routing and paging": "scripts/patches/google_site_search_290716.patch"
          }
    },
    "replace": {
        "drupal/drupal": "*"
    }
}
