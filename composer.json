        "drupal/config_tools": "8.1.x-dev",
        "drupal/contact_block": "8.1.x-dev",
        "drupal/redis": "8.1.x-dev",
        "drupal/google_site_search": "*",
        "drupal/easychart": "8.3.x-dev",
        "drupal/rabbit_hole": "8.1.x-dev",
        "drupal/twig_tweak": "8.1.x-dev",
        "drupal/eu_cookie_compliance": "8.1.x-dev",
        "drupal/contact_storage": "8.1.x-dev",
